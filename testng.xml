<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Selenium Java Architecture Skeleton Suite" parallel="tests" thread-count="2">
    <test name="Firefox Test">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="io.github.burakkaygusuz.tests.FormAuthenticationTest"/>
        </classes>
    </test>
    <test name="Chrome Test">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="io.github.burakkaygusuz.tests.FormAuthenticationTest"/>
        </classes>
    </test>
    <test name="Edge Test">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="io.github.burakkaygusuz.tests.FormAuthenticationTest"/>
        </classes>
    </test>
</suite>